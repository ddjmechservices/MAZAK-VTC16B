o<tool_setter_param_update> sub

  (uses NGCGUI style arg spec)
  (number after "=" in comment is default value)
  #<spindle_zero_height> = #1  (=0.0)
  #<toolsetter_decel_distance> = #2  (=0.15748)
  #<toolsetter_tolerance> = #3  (=0.07874)

  (Cancel G92 offsets)
  G92.1
  
  (Set the spindle_zero_height parameter)
  #3023 = #<spindle_zero_height>
  
  (Set the Mazak toolsetter parameters)
  o100 if [EXISTS[#<_ini[probe]toolsetter_decel_distance>]]
    (Do not change if already set in INI)
  o100 else
    (Save parameter to global variable)
    #<_ini[probe]toolsetter_decel_distance> = #<toolsetter_decel_distance>
  o100 endif
  
  o110 if [EXISTS[#<_ini[probe]toolsetter_tolerance>]]
    (Do not change if already set in INI)
  o110 else
    (Save parameter to global variable)
    #<_ini[probe]toolsetter_tolerance> = #<toolsetter_tolerance>
  o110 endif
  
  (Output the current values for verification)
  (PRINT, Toolsetter Parameters Updated:)
  (PRINT, Spindle Zero Height: #3023)
  (PRINT, Decel Distance: #<_ini[probe]toolsetter_decel_distance>)
  (PRINT, Measurement Tolerance: #<_ini[probe]toolsetter_tolerance>)
  
o<tool_setter_param_update> endsub [1]

M2
