o<carousel-index> sub
	M65 P6 ;brake off
	G4 P.001 ;dwell
	M64 P7 ;start carousel
	G4 P.001 ;dwell
	M66 P16 L3 Q60 ; wait for carousel finished
	
	o105 if [#5399 LT 0]
		(ABORT, FAILED TO ALIGN CAROUSEL!)
	o105 endif
	
	G4 P.001 ;dwell
	M64 P6 ;brake on
	G4 P.1 ;dwell
	M65 P6 ;brake off
	M65 P7 ;stop carousel
o<carousel-index> endsub
M2
